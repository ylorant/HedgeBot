        "php": ">=7.0",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "symfony/console": "^3.3",
        "symfony/event-dispatcher": "^4.0",
        "ylorant/twitch-php-client": "1.3",
        "jublonet/codebird-php": "^3.1",
        "wisembly/elephant.io": "dev-master",
        "ylorant/horaro-php-client": "^1.1",
        "symfony/mercure": "^0.4.0",
